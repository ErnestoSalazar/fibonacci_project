{
  "name": "fibonnaci",
  "version": "1.0.0",
  "description": "monorepo for fibonnaci project",
  "main": "index.js",
  "scripts": {
    "install:backend": "cd fibonacci_backend && npm install",
    "install:client": "cd fibonnaci_client && npm install",
    "install:all": "npm run install:backend && npm run install:client",
    "start": "cd fibonacci_backend && concurrently \"npm start\""
  },
  "author": "Ernesto Salazar",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
